{
  "name": "prompt-refiner",
  "displayName": "Prompt Refiner",
  "version": "1.0.0",
  "publisher": "Hanfried",
  "description": "Refine your prompts instantly with AI using n8n workflows",
  "engines": {
    "vscode": "^1.75.0"
  },
  "type": "module",
  "main": "dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "promptRefiner.refineSelection",
        "title": "Refine Selection",
        "category": "Prompt Refiner"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanfried-Nguegan/AI-Prompt-Refiner-Agent"
  },
  "author": "Hanfried Nguegan",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^20.5.1",
    "@types/vscode": "^1.75.0",
    "typescript": "^5.2.2",
    "vsce": "^2.15.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "bun dist/refine.js",
    "dev": "bun src/refine.ts",
    "daemon": "bun src/daemon.ts",
    "cli": "bun src/refine.ts",
    "package": "vsce package",
    "publish": "vsce publish"
  }
}