{
  "name": "prompt-refiner",
  "displayName": "Prompt Refiner",
  "version": "1.1.0",
  "publisher": "Hanfried",
  "description": "Refine your prompts instantly with AI using n8n workflows",
  "engines": {
    "vscode": "^1.75.0",
    "node": ">=18.0.0"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/lib.d.ts",
  "bin": {
    "prompt-refiner": "./bin/prompt-refiner.js",
    "prompt-refiner-daemon": "./bin/prompt-refiner-daemon.js"
  },
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "promptRefiner.refineSelection",
        "title": "Refine Selection",
        "category": "Prompt Refiner"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Hanfried-Nguegan/AI-Prompt-Refiner-Agent"
  },
  "author": "Hanfried Nguegan",
  "license": "MIT",
  "keywords": [
    "prompt",
    "refiner",
    "ai",
    "n8n",
    "llm",
    "vscode"
  ],
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bun": "^1.3.5",
    "@types/node": "^20.5.1",
    "@types/vscode": "^1.75.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.18.2",
    "vsce": "^2.15.0",
    "vitest": "^1.1.4"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "bun dist/refine.js",
    "dev": "bun src/refine.ts",
    "daemon": "bun src/daemon.ts",
    "cli": "bun src/refine.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "prepublishOnly": "npm run clean && npm run build",
    "package": "vsce package",
    "publish": "vsce publish"
  }
}